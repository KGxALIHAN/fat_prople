{
  "venvPath": "./.venv",
  "pythonVersion": "3.8",
  "include": ["src"],
  "exclude": ["node_modules", ".git"],
  "reportMissingImports": true,
  "reportGeneralTypeIssues": true,
  "pythonPath": "./.venv/bin/python",
